  </ItemGroup>
  <ItemGroup>
    <Compile Update="Forms\AddUpdateMatch.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\AddUpdateTournaments.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\ChangePassForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\ConfigDatabase.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\ToggleAdmin.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\ToggleUser.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\TournamentsForm.cs">
      <SubType>Form</SubType>
