    <Compile Update="Forms\UserInfoForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\UpdateTime.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\AddUpdateMatchsets.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\AdminForm.cs">
