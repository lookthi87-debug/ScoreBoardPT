    <Compile Update="Forms\UserForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\AddUserDialog.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\LoginForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\MatchsForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Data\PostgresHelper.cs">
